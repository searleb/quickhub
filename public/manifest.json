{
  "short_name": "React App",
  "name": "QuickHub",
  "description": "QuickHub for GitHub",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self';"
}
